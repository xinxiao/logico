local pin = import "@pin.lib";
local connector = import "@connector.lib";

{
    "name": "nand",

    "nodes": {
        "and": "and",
        "not": "not",
    },

    "inputs": {
        "a": [pin.Pin("and", "a")],
        "b": [pin.Pin("and", "b")],
    },

    "connectors": [
        connector.Edge(pin.Pin("and", "out"), pin.Pin("not", "v")),
    ],

    "outputs": {
        "out": pin.Pin("not", "out"),
    },
}